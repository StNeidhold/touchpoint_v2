<div class="page-header">
  <h1>
    Ib groups
    <a href="/ib_groups/new" class="btn btn-lg btn-success">New Ib group</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Name</th>
        <th>Actions</th>
      </tr>

      <% @ib_groups.each do |ib_group| %>
      <tr>
        <td><%= ib_group.name %></td>
        <td>
          <a href="/ib_groups/<%= ib_group.id %>" class="btn btn-primary">Show</a>
          <a href="/ib_groups/<%= ib_group.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_ib_group/<%= ib_group.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
