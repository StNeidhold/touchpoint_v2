<div class="page-header">
  <h3>Ib Group #<%= @ib_group.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>Name</dt>
      <dd><%= @ib_group.name %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/ib_groups" class="btn btn-primary">
        Back
      </a>
      <a href="/ib_groups/<%= @ib_group.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_ib_group/<%= @ib_group.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
