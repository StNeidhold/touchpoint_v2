<div class="page-header">
  <h1>
    Firm offices
    <a href="/firm_offices/new" class="btn btn-lg btn-success">New Firm office</a>
  </h1>
</div>

<div class="row">
  <div class="col-md-12">
    <table class="table table-striped table-hover">
      <tr>
        <th>Firm</th>
        <th>City</th>
        <th>Integer</th>
        <th>Actions</th>
      </tr>

      <% @firm_offices.each do |firm_office| %>
      <tr>
        <td><%= firm_office.firm_id %></td>
        <td><%= firm_office.city_id %></td>
        <td><%= firm_office.integer %></td>
        <td>
          <a href="/firm_offices/<%= firm_office.id %>" class="btn btn-primary">Show</a>
          <a href="/firm_offices/<%= firm_office.id %>/edit" class="btn btn-warning">Edit</a>
          <a href="/delete_firm_office/<%= firm_office.id %>" class="btn btn-danger" rel="nofollow">Delete</a>
        </td>
      </tr>
      <% end %>
    </table>
  </div>
</div>
