<div class="page-header">
  <h3>Event Attendee #<%= @event_attendee.id %></h3>
</div>

<div class="row">
  <div class="col-md-12 mb-2">
    <dl class="dl-horizontal">
      <dt>User</dt>
      <dd><%= @event_attendee.user_id %></dd>

      <dt>Recruiting event</dt>
      <dd><%= @event_attendee.recruiting_event_id %></dd>

    </dl>

    <div class="btn-group btn-group-justified">
      <a href="/event_attendees" class="btn btn-primary">
        Back
      </a>
      <a href="/event_attendees/<%= @event_attendee.id %>/edit" class="btn btn-warning">
        Edit
      </a>
      <a href="/delete_event_attendee/<%= @event_attendee.id %>" class="btn btn-danger" rel="nofollow">
        Delete
      </a>
    </div>
  </div>
</div>
